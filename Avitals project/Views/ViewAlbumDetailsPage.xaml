<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Avitals_project.Views.ViewAlbumDetailsPage"
             xmlns:viewmodels="clr-namespace:Avitals_project.ViewModels"
             Title="ViewAlbumDetailsPage"
             BackgroundColor="{StaticResource MainColor}">
    <VerticalStackLayout>
        <Grid RowDefinitions="20,*" ColumnDefinitions="Auto,*,Auto" Padding="40,70,40,30">
            <HorizontalStackLayout Grid.Row="0">
                <Border HeightRequest="60" 
                WidthRequest="60"
                StrokeShape="RoundRectangle 30,30,30,30"
                HorizontalOptions="Center"
                
                StrokeThickness="8"
                Margin="0,0,0,0">
                   
                    </Border>
                </HorizontalStackLayout>
            </Grid>
    </VerticalStackLayout>
</ContentPage>